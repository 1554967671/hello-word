/* pages/city/city.wxss */
page{
  background-color: #F0F0F0;
}
.showcity{
  position: absolute;
  height: 100%;
}
.title{
  float: left;
  width: 100%;
  height: 30px;
  font-size: 14px;
  padding-left: 13px;
  line-height: 30px;
  background: #E0E0E0;
}
.pad_title{
  margin-top: 15px;
}

.cityname{
  float: left;
  margin-left: 13px;
  margin-top: 15px;
  width:200rpx;
  height: 36px;
  background: white;
  text-align: center;
  line-height: 36px;
  font-size: 14px;
}

.citydata{
  float: left;
  width: 660rpx;
  height: 50px;
  margin-left: 13px;
  font-size: 14px;
  line-height: 50px;
  border-bottom: 1px solid #E0E0E0;
}

.choose_cate{
  position: fixed;
  top: 60px;
  right: 8px;
  width: 30px;
  height: 528px;
}
.cate{
  font-size: 14px;
  margin-top: 2px;
}
.letlist{
  text-align: center;
}














